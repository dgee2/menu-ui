{
  "name": "MenuUI",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run build-prerequisites && vite",
    "build": "yarn run build-prerequisites && vite build",
    "preview": "yarn run build-prerequisites && vite preview",
    "build-prerequisites": "yarn run generate-openapi && vue-tsc --noEmit",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "generate-openapi": "openapi-typescript ./open-api/*.yaml --output ./src/generated/",
    "test": "vitest",
    "dev-test": "vitest --ui"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@qdrant/openapi-typescript-fetch": "^1.2.6",
    "pinia": "^2.3.0",
    "roboto-fontface": "*",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.6",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.26.3",
    "@types/node": "^22.10.2",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^2.1.8",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "openapi-typescript": "^7.4.4",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
