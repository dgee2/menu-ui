{
  "name": "MenuUI",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run build-prerequisites && vite",
    "build": "yarn run build-prerequisites && vite build",
    "preview": "yarn run build-prerequisites && vite preview",
    "build-prerequisites": "yarn run generate-openapi && vue-tsc --noEmit",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "generate-openapi": "openapi-typescript ./open-api/*.yaml --output ./src/generated/",
    "test": "vitest",
    "dev-test": "vitest --ui"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@qdrant/openapi-typescript-fetch": "^1.2.6",
    "pinia": "^2.2.2",
    "roboto-fontface": "*",
    "vue": "^3.5.0",
    "vue-router": "^4.4.3",
    "vuetify": "^3.7.1",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@types/node": "^22.5.2",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/ui": "^2.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.0",
    "openapi-typescript": "^7.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.4"
  }
}
