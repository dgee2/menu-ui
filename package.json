{
  "name": "MenuUI",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run build-prerequisites && vite",
    "build": "yarn run build-prerequisites && vite build",
    "preview": "yarn run build-prerequisites && vite preview",
    "build-prerequisites": "yarn run generate-openapi && vue-tsc --noEmit",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "generate-openapi": "openapi-typescript ./open-api/*.yaml --output ./src/generated/",
    "test": "vitest",
    "dev-test": "vitest --ui"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@qdrant/openapi-typescript-fetch": "^1.2.6",
    "pinia": "^2.1.7",
    "roboto-fontface": "*",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.12",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@types/node": "^20.5.1",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/ui": "^2.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.6.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.0",
    "openapi-typescript": "^7.0.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^2.0.1",
    "vue-tsc": "^2.0.26"
  }
}
