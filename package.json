{
  "name": "MenuUI",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn run build-prerequisites && vite",
    "build": "yarn run build-prerequisites && vite build",
    "preview": "yarn run build-prerequisites && vite preview",
    "build-prerequisites": "yarn run generate-openapi && vue-tsc --noEmit",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "generate-openapi": "openapi-typescript ./open-api/*.yaml --output ./src/generated/",
    "test": "vitest",
    "dev-test": "vitest --ui"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@qdrant/openapi-typescript-fetch": "^1.2.1",
    "pinia": "^2.1.7",
    "roboto-fontface": "*",
    "vue": "^3.4.18",
    "vue-router": "^4.2.5",
    "vuetify": "^3.5.3",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@types/node": "^20.5.1",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^1.2.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "jsdom": "^24.0.0",
    "openapi-typescript": "^6.7.4",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-plugin-vuetify": "^2.0.1",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  }
}
